<?xml version="1.0"?>

<PropertyList>

<path>enginepanel2.ac</path>

<animation>
  <object-name>Temp1</object-name>
  <object-name>Temp2</object-name>
  <object-name>OilPressL1</object-name>
  <object-name>OilPressL2</object-name>
  <object-name>OilPressR1</object-name>
  <object-name>OilPressR2</object-name>
  <object-name>OilTempL1</object-name>
  <object-name>OilTempL2</object-name>
  <object-name>OilTempR1</object-name>
  <object-name>OilTempR2</object-name>
  <object-name>OilQTYL1</object-name>
  <object-name>OilQTYL2</object-name>
  <object-name>OilQTYR1</object-name>
  <object-name>OilQTYR2</object-name>
  <object-name>HydPressL1</object-name>
  <object-name>HydPressL2</object-name>
  <object-name>HydPressR1</object-name>
  <object-name>HydPressR2</object-name>
  <object-name>HydQTYL1</object-name>
  <object-name>HydQTYL2</object-name>
  <object-name>HydQTYR1</object-name>
  <object-name>HydQTYR2</object-name>
  <object-name>FlapL</object-name>
  <object-name>FlapR</object-name>
  <emission>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
  </emission>
</animation>

<!-- Power on/off -->
<animation>
	<name>Power on/off</name>
	<type>select</type>
	<object-name>Temp1</object-name>
	<object-name>Temp2</object-name>
	<object-name>FuelTempL1</object-name>
	<object-name>FuelTempL2</object-name>
	<object-name>FuelTempR1</object-name>
	<object-name>FuelTempR2</object-name>
	<object-name>OilPressL1</object-name>
	<object-name>OilPressL2</object-name>
	<object-name>OilPressR1</object-name>
	<object-name>OilPressR2</object-name>
	<object-name>OilTempL1</object-name>
	<object-name>OilTempL2</object-name>
	<object-name>OilTempR1</object-name>
	<object-name>OilTempR2</object-name>
	<object-name>OilQTYL1</object-name>
	<object-name>OilQTYL2</object-name>
	<object-name>OilQTYR1</object-name>
	<object-name>OilQTYR2</object-name>
	<object-name>HydPressL1</object-name>
	<object-name>HydPressL2</object-name>
	<object-name>HydPressR1</object-name>
	<object-name>HydPressR2</object-name>
	<object-name>HydQTYL1</object-name>
	<object-name>HydQTYL2</object-name>
	<object-name>HydQTYR1</object-name>
	<object-name>HydQTYR2</object-name>
	<object-name>FlapL</object-name>
	<object-name>FlapR</object-name>
	<condition>
		<or>
		<greater-than>
			<property>systems/electrical/bus/dcL</property>
			<value>15</value>
		</greater-than>
		<greater-than>
			<property>systems/electrical/bus/dcR</property>
			<value>15</value>
		</greater-than>
		</or>
	</condition>
</animation>

<!-- Temperature Celcius -->
<animation>
  <type>textranslate</type>
  <object-name>Temp1</object-name>
  <property>/environment/temperature-degc</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>Temp2</object-name>
  <property>/environment/temperature-degc</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<!-- Fuel Temp -->
<animation>
  <type>textranslate</type>
  <object-name>FuelTempL1</object-name>
  <!--property>/systems/fuel/temperatureR</property--> <!-- Will be changed when I add the fuel system, and anti-ice system -->
  <property>/environment/temperature-degc</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>FuelTempL2</object-name>
  <!--property>/systems/fuel/temperatureR</property--> <!-- Will be changed when I add the fuel system, and anti-ice system -->
  <property>/environment/temperature-degc</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>FuelTempR1</object-name>
  <!--property>/systems/fuel/temperatureR</property--> <!-- Will be changed when I add the fuel system, and anti-ice system -->
  <property>/environment/temperature-degc</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>FuelTempR2</object-name>
  <!--property>/systems/fuel/temperatureR</property--> <!-- Will be changed when I add the fuel system, and anti-ice system -->
  <property>/environment/temperature-degc</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<!-- Oil Pressure -->
<animation>
  <type>textranslate</type>
  <object-name>OilPressL1</object-name>
  <property>/engines/engine[0]/oil-pressure</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>OilPressL2</object-name>
  <property>/engines/engine[0]/oil-pressure</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>OilPressR1</object-name>
  <property>/engines/engine[1]/oil-pressure</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>OilPressR2</object-name>
  <property>/engines/engine[1]/oil-pressure</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<!-- Flaps -->
<animation>
  <type>translate</type>
  <object-name>FlapL</object-name>
  <property>/surface-positions/flap-pos-norm</property>
  <interpolation>
	<entry><ind>00000</ind><dep>0.0000</dep></entry>
	<entry><ind>0.125</ind><dep>0.0075</dep></entry>
	<entry><ind>0.275</ind><dep>0.0165</dep></entry>
	<entry><ind>0.375</ind><dep>0.0225</dep></entry>
	<entry><ind>0.700</ind><dep>0.0423</dep></entry>
	<entry><ind>1.000</ind><dep>0.0480</dep></entry>
  </interpolation>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>-1</z>
  </axis>
</animation>

<animation>
  <type>translate</type>
  <object-name>FlapR</object-name>
  <property>/surface-positions/flap-pos-norm</property>
  <interpolation>
	<entry><ind>00000</ind><dep>0.0000</dep></entry>
	<entry><ind>0.125</ind><dep>0.0075</dep></entry>
	<entry><ind>0.275</ind><dep>0.0165</dep></entry>
	<entry><ind>0.375</ind><dep>0.0225</dep></entry>
	<entry><ind>0.700</ind><dep>0.0423</dep></entry>
	<entry><ind>1.000</ind><dep>0.0480</dep></entry>
  </interpolation>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>-1</z>
  </axis>
</animation>

</PropertyList>

