<!--  AutoThrottle Switch -->

<animation>
  <type>select</type>
  <object-name>ATSwitchOn</object-name>
  <condition>
    <property>controls/switches/at</property>
  </condition>
</animation>
<animation>
  <type>pick</type>
  <object-name>ATSwitchOn</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
	<command>property-assign</command>
        <property>controls/switches/at</property>
	<value>0</value>
      <!--command>nasal</command>
      <script>MD90.autothrottle_toggle();</script-->
    </binding>
    <binding>
	<command>property-assign</command>
        <property>autopilot/locks/speed</property>
	<value></value>
    </binding>
  </action>
</animation>

<animation>
  <type>select</type>
  <object-name>ATSwitchOff</object-name>
  <condition>
    <not><property>controls/switches/at</property></not>
  </condition>
</animation>
<animation>
  <type>pick</type>
  <object-name>ATSwitchOff</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
	<command>property-assign</command>
        <property>controls/switches/at</property>
	<value>1</value>
      <!--command>nasal</command>
      <script>MD90.autothrottle_toggle();</script-->
    </binding>
    <binding>
	<condition><equals>
	<property>autopilot/locks/at-mode</property>
	<value>0</value></equals>
	</condition>
	<command>property-assign</command>
        <property>autopilot/locks/speed</property>
	<value>speed-with-throttle</value>
    </binding>
    <binding>
	<condition><equals>
	<property>autopilot/locks/at-mode</property>
	<value>1</value></equals>
	</condition>
	<command>property-assign</command>
        <property>autopilot/locks/speed</property>
	<value>mach-with-throttle</value>
    </binding>
  </action>
</animation>

<!--  AutoPilot Switch -->

<animation>
  <type>select</type>
  <object-name>APSwitchOn</object-name>
  <condition>
    <property>/controls/switches/ap_master</property>
  </condition>
</animation>
<animation>
  <type>pick</type>
  <object-name>APSwitchOn</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
	<command>property-assign</command>
        <property>/controls/switches/ap_master</property>
	<value>0</value>
    </binding>
    <binding>
	<command>nasal</command>
    <script>MD90.ap_off();</script>
    </binding>
  </action>
</animation>

<animation>
  <type>select</type>
  <object-name>APSwitchOff</object-name>
  <condition>
    <not><property>/controls/switches/ap_master</property></not>
  </condition>
</animation>
<animation>
  <type>pick</type>
  <object-name>APSwitchOff</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
	<command>property-assign</command>
        <property>/controls/switches/ap_master</property>
	<value>1</value>
    </binding>
    <binding>
	<command>nasal</command>
    <script>MD90.ap_refresh();</script>
    </binding>
  </action>
</animation>

<!--  AutoThrottle Knob (Speed selected) -->

<animation>
  <type>rotate</type>
  <object-name>ATKnob</object-name>
  <property>/autopilot/settings/target-speed-kt</property>
  <interpolation>
    <entry><ind>  0</ind><dep>  0</dep></entry>
    <entry><ind>300</ind><dep>720</dep></entry>
  </interpolation>
  <axis>
    <x1-m>2.498584</x1-m>
    <y1-m>-0.107</y1-m>
    <z1-m>1.157239</z1-m>
    <x2-m>2.488774</x2-m>
    <y2-m>-0.107</y2-m>
    <z2-m>1.155154</z2-m>
  </axis>
</animation>

<animation>
