	<!-- Power on/off -->
	<animation>
		<name>Power on/off</name>
		<type>select</type>
		<object-name>Off</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/efis</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation>
<animation>
  <type>select</type>
  <object-name>ATDigit1</object-name>
  <object-name>ATDigit2</object-name>
  <object-name>ATDigit3</object-name>
  <object-name>ATKnob</object-name>
  <condition>
    <equals>
      <property>autopilot/locks/at-mode</property>
      <value>0</value>
    </equals>
  </condition>
</animation>
<animation>
  <type>select</type>
  <object-name>MachDigit1</object-name>
  <object-name>MachDigit2</object-name>
