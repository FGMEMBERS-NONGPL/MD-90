<!--  AutoThrottle Switch -->

<animation>
  <type>select</type>
  <object-name>ATSwitchOn</object-name>
  <condition>
    <property>/it-autoflight/input/athr</property>
  </condition>
</animation>
<animation>
  <type>pick</type>
  <object-name>ATSwitchOn</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
			<binding>
			<command>property-assign</command>
			<property>/it-autoflight/input/athr</property>
			<value>0</value>
			</binding>
  </action>
</animation>

<animation>
  <type>select</type>
  <object-name>ATSwitchOff</object-name>
  <condition>
    <not><property>/it-autoflight/input/athr</property></not>
  </condition>
</animation>
<animation>
  <type>pick</type>
  <object-name>ATSwitchOff</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
			<binding>
			<command>property-assign</command>
			<property>/it-autoflight/input/athr</property>
			<value>1</value>
			</binding>
  </action>
</animation>

<!--  AutoPilot Switch -->

<animation>
  <type>select</type>
  <object-name>APSwitchOn</object-name>
  <condition>
	<or>
      <property>/it-autoflight/input/ap1</property>
      <property>/it-autoflight/input/ap2</property>
	</or>
  </condition>
</animation>
<animation>
  <type>pick</type>
  <object-name>APSwitchOn</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
	<command>property-assign</command>
        <property>/it-autoflight/input/ap1</property>
	<value>0</value>
    </binding>
    <binding>
	<command>property-assign</command>
        <property>/it-autoflight/input/ap2</property>
	<value>0</value>
    </binding>
  </action>
</animation>

<animation>
  <type>select</type>
  <object-name>APSwitchOff</object-name>
  <condition>
	<and>
      <not><property>/it-autoflight/input/ap1</property></not>
      <not><property>/it-autoflight/input/ap2</property></not>
	</and>
  </condition>
</animation>
<animation>
  <type>pick</type>
  <object-name>APSwitchOff</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
	<command>property-assign</command>
        <property>/it-autoflight/input/ap1</property>
	<value>1</value>
    </binding>
  </action>
</animation>

