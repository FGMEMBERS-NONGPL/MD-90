      <value>1</value>
    </equals>
  </condition>
</animation>

<!--  AutoThrottle Digits -->

<animation>
  <type>textranslate</type>
  <object-name>ATDigit1</object-name>
  <property>/autopilot/settings/target-speed-kt</property>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>ATDigit2</object-name>
  <property>/autopilot/settings/target-speed-kt</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>ATDigit3</object-name>
  <property>/autopilot/settings/target-speed-kt</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<!--  Mach digits -->

