 <animation>
        <type>select</type>
        <object-name>CTRLND.flare</object-name>
        <condition>
            <and>
                <equals>
                    <property>controls/lighting/landing-lights</property>
                    <value>true</value>
                </equals>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>1</value>
                </equals>
            </and>
        </condition>
    </animation>

<animation>
        <type>select</type>
        <object-name>CTRLND.spot</object-name>
        <condition>
            <and>
		<not><property>sim/rendering/rembrandt/enabled</property></not>
                <equals>
                    <property>controls/lighting/landing-lights</property>
                    <value>true</value>
                </equals>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>1</value>
                </equals>
                <greater-than>
                    <property>sim/time/sun-angle-rad</property>
                    <value>1.55</value>
                </greater-than>
            </and>
        </condition>
    </animation>


