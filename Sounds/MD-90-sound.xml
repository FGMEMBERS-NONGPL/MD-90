        </position>
        <reference-dist>60.0</reference-dist>
        <max-dist>50.0</max-dist>
    </engine>


<engine>
	<name>de ice truck operation</name>
	<path>/Aircraft/MD-90/Sounds/electricalengine.wav</path>
	<mode>looped</mode>
        <condition>
            <less-than>	
	         <property>/velocities/groundspeed-kt</property>
	         <value>1</value>
	         </less-than>
        </condition>
        <volume>
            <property>/services/deicing_truck/de-ice</property>
            <value>1</value>
        </volume>
        <!--volume>
            <property>sim/sound/Ovolume</property>
        </volume-->
        <position>
            <x-m>-15</x-m>
		<y-m>9.5</y-m>
		<z-m>-3.45</z-m>
        </position>
        <reference-dist>60.0</reference-dist>
        <max-dist>50.0</max-dist>
    </engine>



