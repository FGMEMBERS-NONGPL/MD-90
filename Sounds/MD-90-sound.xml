<?xml version="1.0"?>
 
<PropertyList>

<fx>

  <rumble>
   <name>rumble</name>
   <mode>looped</mode>
   <path>/Aircraft/MD-90/Sounds/rumble.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/groundspeed-kt</property>
    <type>log</type>
    <factor>0.35</factor>
   </volume>
  </rumble>


  <gear>
   <name>gear</name>
   <mode>in-transit</mode>
   <path>/Aircraft/MD-90/Sounds/gear.wav</path>
   <property>/gear/gear[0]/position-norm</property>
   <pitch>
    <internal>dt_play</internal>
    <offset>1.0</offset>
    <factor>-0.001</factor>
   </pitch>
  </gear>

  <gear>
   <name>gear-lock</name>
   <path>Sounds/gear-lck.wav</path>
   <condition>
    <equals>
     <property>/gear/gear[0]/position-norm</property>
     <value>0</value>
    </equals>
   </condition>
  </gear>

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/speed-down-fps</property>
    <factor>0.05</factor>
   </volume>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>
 
  <flap>
   <name>flaps</name>
   <mode>in-transit</mode>
   <path>/Aircraft/MD-90/Sounds/flaps.wav</path>
   <property>/surface-positions/flap-pos-norm</property>
   <volume>
    <factor>0.45</factor>
   </volume>
  </flap>

  <wind>
   <name>wind</name>
   <mode>looped</mode>
   <path>Sounds/wind.wav</path>
   <property>/velocities/airspeed-kt</property>
   <volume>
    <property>/position/altitude-ft</property>
    <factor>-0.000015</factor>
    <offset>1.0</offset>
    <min>0.1</min>
    <max>1.0</max>
   </volume>
   <volume>
     <property>/velocities/airspeed-kt</property>
     <factor>0.0001</factor>
     <min>0.0</min>
     <max>0.1</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
    <offset>1.25</offset>
   </pitch>
  </wind>

  <!--stall>
   <name>stall</name>
   <path>/Aircraft/MD-90/Sounds/stall.wav</path>
   <property>/sim/alarms/stall-warning</property>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <min>30.0</min>
   </volume>
  </stall-->

<stall>
   <name>stall</name>
   <mode>looped</mode>
   <path>/Aircraft/MD-90/Sounds/stall.wav</path>
   <property>/sim/alarms/stall-warning</property>
   <volume>
    <property>/sim/alarms/stall-warning</property>
    <max>1.0</max>
   </volume>
  </stall>

  <engine>
    <name>left-thrust</name>
    <path>Sounds/jet.wav</path>
    <condition>
      <not><property>/engines/engine[0]/out-of-fuel</property></not>
    </condition>
    <mode>looped</mode>
    <volume>
      <property>engines/engine[0]/thrust-lbs</property>
      <factor>0.00001</factor>
    </volume>
  </engine>
  <engine>
    <name>left-n1</name>
    <path>Sounds/whine.wav</path>
    <condition>
      <not><property>/engines/engine[0]/out-of-fuel</property></not>
    </condition>
    <mode>looped</mode>
    <volume>
      <property>engines/engine[0]/n1-actual</property>
      <factor>0.005</factor>
    </volume>
    <pitch>
      <property>/engines/engine[0]/n1-actual</property>
      <offset>0</offset>
      <factor>0.01</factor>
    </pitch>
  </engine>
  <engine>
    <name>right-thrust</name>
    <path>Sounds/jet.wav</path>
    <condition>
      <not><property>/engines/engine[1]/out-of-fuel</property></not>
    </condition>
    <mode>looped</mode>
    <volume>
      <property>engines/engine[1]/thrust-lbs</property>
      <factor>0.00001</factor>
    </volume>
  </engine>
  <engine>
    <name>right-n1</name>
    <path>Sounds/whine.wav</path>
    <condition>
      <not><property>/engines/engine[1]/out-of-fuel</property></not>
    </condition>
    <mode>looped</mode>
    <volume>
      <property>engines/engine[1]/n1-actual</property>
      <factor>0.005</factor>
    </volume>
    <pitch>
      <property>/engines/engine[1]/n1-actual</property>
      <offset>0</offset>
      <factor>0.01</factor>
    </pitch>
  </engine>

<!-- Services-->
<engine>
	<name>fuel truck pump transfer</name>
	<path>/Aircraft/MD-90/Sounds/electricalengine.wav</path>
	<mode>looped</mode>
        <condition>
            	<less-than>	
	         <property>/velocities/groundspeed-kt</property>
	         <value>1</value>
	         </less-than>
        </condition>
        <volume>
            <property>/services/fuel-truck/transfer</property>
            <value>1</value>
        </volume>
        <!--volume>
            <property>sim/sound/Ovolume</property>
        </volume-->
        <position>
            <x-m>0.25</x-m>
     	 	<y-m>-1.0</y-m>
     		 <z-m>1.0</z-m>
        </position>
        <reference-dist>60.0</reference-dist>
        <max-dist>50.0</max-dist>
    </engine>

  <engine>
	<name>fuel truck pump clean</name>
	<path>/Aircraft/MD-90/Sounds/electricalengine.wav</path>
