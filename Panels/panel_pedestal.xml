<PropertyList>

<name>Pedestal Panel</name>
<background>Aircraft/MD-90/Panels/transparent-bg.rgb</background>
<w>512</w>
<h>512</h>

<instruments>

  <instrument>
    <name>panel hotspots</name>
    <x>256</x>
    <y>256</y>
    <w>512</w>
    <h>512</h>
