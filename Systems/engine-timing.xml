        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input> 
            <expression>
                <table>
                    <property>/engines/engine[0]/fuel-flow-pph</property> 
                    <entry><ind>0</ind><dep>0</dep></entry>
                    <entry><ind>0.1</ind><dep>2</dep></entry>
                    <entry><ind>12000</ind><dep>12</dep></entry>
                </table>
            </expression>
        </input>
		<output>/engines/engine[0]/fuel-flow-actual</output>
    </filter>
	
